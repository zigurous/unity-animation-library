{
  "metadata": [
    {
      "src": [
        {
          "src": "../Runtime",
          "files": [
            "**.cs"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "globalNamespaceId": "Global",
      "filter": "filterconfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**.yml"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**.yml",
          "changelog/**.md",
          "changelog/**.yml",
          "license/**.md",
          "license/**.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src": "../",
        "dest": "changelog",
        "files": [
          "CHANGELOG.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "logo.svg",
          "index.html"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "dist/**"
        ]
      }
    ],
    "dest": "dist",
    "globalMetadata": {
      "_appTitle": "Animation Library",
      "_appFooter": "Copyright Â© 2021 Zigurous<br />Generated by <b>DocFX</b>",
      "_appLogoPath": "logo.svg",
      "_appFaviconPath": "favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_gitContribute": {
        "repo": "https://github.com/zigurous/unity-animation-library",
        "branch": "main"
      }
    },
    "fileMetadata": {
      "_disableAffix": {
        "license/**.md": true
      }
    },
    "template": [
      "default",
      "templates/zigurous"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false
  }
}
